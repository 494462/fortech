<% if(errors.length > 0){ %>
    <div class="notification is-danger">
        <% errors.forEach(erro => { %>
            <%= erro %><br>
        <% }) %>
    </div>
<% } %>

<% if(success.length > 0){ %>
    <div class="notification is-primary">
        <% success.forEach(success => { %>
            <%= success %><br>
        <% }) %>
    </div>
<% } %>